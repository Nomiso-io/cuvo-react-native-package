  "license": "MIT",
  "files": [
    "android-exoplayer",
    "android",
    "ios",
    "lib",
    "index.d.ts",
    "fileTypes.js",
    "fileTypes.d.ts",
    "audioRecoder.js",
    "audioRecoder.d.ts",
